<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" >
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <size android:width="20dip"
                android:height="10dip"/><!--大小-->
            <solid android:color="@color/transparent"/><!--填充-->
            <stroke android:width="2dip" android:color="@color/aurora_grey"/><!--描边-->
            <corners android:radius="30dip"/><!--圆角-->
        </shape>
    </item>
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <size android:width="20dip"
                android:height="10dip"/><!--大小-->
            <solid android:color="@color/aurora_grey"/><!--填充-->
            <corners android:radius="30dip"/><!--圆角-->
        </shape>
    </item>
</selector>